<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qidu.lin.time.accumulater"
    android:versionCode="7"
    android:versionName="0.16" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme"
        android:label="@string/app_name" >
        <activity
            android:name="com.qidu.lin.time.accumulater.fg.TAMain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.qidu.lin.time.accumulater.fg.TAProjectListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.qidu.lin.time.accumulater.fg.TAClockwiseTomatoSystemAlarmReceiver"
            android:label="@string/title_activity_intent_filter_receiver" >
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>
        <activity android:name="com.qidu.lin.time.accumulater.fg.TATomatoListActivity" >
        </activity>
        <activity
            android:name="com.qidu.lin.time.accumulater.fg.TAManualRecord"
            android:label="@string/title_activity_tamanual_record" >
        </activity>
    </application>

</manifest>