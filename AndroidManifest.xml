<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qidu.lin.time.accumulater"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="com.qidu.lin.time.accumulater.TAMain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.qidu.lin.time.accumulater.TASelect"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.qidu.lin.time.accumulater.TAClockwiseTomatoReceiver"
            android:label="@string/title_activity_intent_filter_receiver" >
            <intent-filter > 
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/plain" /> 
                </intent-filter>
        </activity>
    </application>

</manifest>